{
  "name": "koa2-boilerplate",
  "version": "1.0.0",
  "description": "koa2 MVC boilerplate",
  "main": "app.js",
  "dependencies": {
    "joi": "^11.3.4",
    "koa": "^2.3.0",
    "koa-body": "^2.5.0",
    "koa-logger": "^3.1.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.2.1",
    "mongoose": "^4.12.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
