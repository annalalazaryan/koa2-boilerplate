{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["README.md", "app.js", "package.json", "api/", "libs/"],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "templates/default",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true,
        "encoding": "utf8",
        "private": false,
        "readme": "./README.md",
        "package": "./package.json"
    },
    "templates": {
    }
}